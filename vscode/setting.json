{
    "http.proxySupport": "off",
    "terminal.integrated.localEchoLatencyThreshold": -1,
    "terminal.integrated.localEchoEnabled": "off",
    "workbench.editor.enablePreview": false,
    "editor.minimap.enabled": false,
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        // buffer
        {
            "before": [
                "leader",
                "b"
            ],
            "when": [
                "!activeEditorGroupEmpty"
            ],
            "commands": [
                "workbench.action.quickOpenLeastRecentlyUsedEditorInGroup"
            ]
        },
        // file tree
        {
            "before": [
                "<leader>",
                "e"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        // rename file
        {
            "before": [
                "<leader>",
                "r",
                "f"
            ],
            "commands": [
                "fileutils.renameFile"
            ]
        },
        // find file regex
        {
            "before": [
                "f",
                "g"
            ],
            "commands": [
                "workbench.view.search"
            ]
        },
        // find file
        {
            "before": [
                "f",
                "f"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        // format
        {
            "before": [
                "<C-f>"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "da"
            ],
            "after": [
                "ggVGd"
            ]
        },
        {
            "before": [
                ";"
            ],
            "after": [
                ":"
            ]
        },
        {
            "before": [
                "leader",
                "i"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "leader",
                "a"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "lineBreakInsert"
            ],
            "silent": true
        },
        {
            "before": [
                "leader",
                "w"
            ],
            "commands": [
                "workbench.action.save"
            ],
            "silent": true
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "leader",
                "w"
            ],
            "commands": [
                "workbench.action.files.save"
            ]
        },
        {
            "before": [
                "leader",
                ";"
            ],
            "commands": [
                "workbench.action.showCommands"
            ],
            "silent": true
        },
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-Space>": false,
        "<C-b>": false,
        "<C-f>": false,
        "<C-e>": false,
        "<C-Shift-p>": false,
    },
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": [
        "#8FBCBB",
        "#434C5E"
    ],
    "vim.statusBarColors.insert": "#BF616A",
    "vim.statusBarColors.visual": "#B48EAD",
    "vim.statusBarColors.visualline": "#B48EAD",
    "vim.statusBarColors.visualblock": "#A3BE8C",
    "vim.statusBarColors.replace": "#D08770",
    "vim.statusBarColors.commandlineinprogress": "#007ACC",
    "vim.statusBarColors.searchinprogressmode": "#007ACC",
    "vim.statusBarColors.easymotionmode": "#007ACC",
    "vim.statusBarColors.easymotioninputmode": "#007ACC",
    "vim.statusBarColors.surroundinputmode": "#007ACC",
    "files.autoSave": "off",
    "workbench.colorCustomizations": {
        "statusBar.background": "#B48EAD",
        "statusBar.noFolderBackground": "#B48EAD",
        "statusBar.debuggingBackground": "#B48EAD",
        "statusBar.foreground": "#434C5E",
        "statusBar.debuggingForeground": "#434C5E"
    },
    "window.menuBarVisibility": "compact",
    "workbench.statusBar.visible": false,
    "workbench.activityBar.location": "hidden",
    "workbench.editor.showTabs": "none"
}